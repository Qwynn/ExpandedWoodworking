<Resources>
 
<!-- Wood Trees --> 
 
	<ThingDef ParentName="ResourceVerbBase">
		<defName>WoodLog_Oak</defName>
		<label>oak logs</label>
		<description>Raw wood from trees or other fibrous plants. When processed it becomes useful for building structures, weapons, and even simple prosthetics. A raw stick of wood can be used as a weapon in a pinch.</description>
		<graphicData>
			<texPath>Resources/WoodLog</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
			<color>(181,137,88)</color>
		</graphicData>
		<equippedAngleOffset>-20</equippedAngleOffset>
		<soundInteract>Wood_Drop</soundInteract>
		<soundDrop>Wood_Drop</soundDrop>
		<statBases>
			<MarketValue>1.4</MarketValue>
			<MaxHitPoints>150</MaxHitPoints>
			<Mass>0.4</Mass>
			<Flammability>1.0</Flammability>
			<DeteriorationRate>2</DeteriorationRate>
			<MeleeWeapon_DamageAmount>8</MeleeWeapon_DamageAmount>
			<MeleeWeapon_Cooldown>2.2</MeleeWeapon_Cooldown>
			<SharpDamageMultiplier>0.3</SharpDamageMultiplier>
			<BluntDamageMultiplier>0.8</BluntDamageMultiplier>
		</statBases>
		<stuffProps>
			<stuffAdjective>oak wood</stuffAdjective>	
			<commonality>1.00</commonality>
			<color>(181,137,88)</color>
			<constructEffect>ConstructWood</constructEffect>
			<appearance>Planks</appearance>
			<soundImpactStuff>BulletImpactWood</soundImpactStuff>
			<soundMeleeHitSharp>MeleeHit_Wood</soundMeleeHitSharp>
			<soundMeleeHitBlunt>MeleeHit_Wood</soundMeleeHitBlunt>
			<statOffsets>
				<Beauty>2</Beauty>
			</statOffsets>
			<statFactors>
				<MaxHitPoints>0.75</MaxHitPoints>
				<Beauty>0.75</Beauty>
				<Flammability>1</Flammability>
				<WorkToMake>0.75</WorkToMake>
				<WorkToBuild>0.7</WorkToBuild>
				<DoorOpenSpeed>0.75</DoorOpenSpeed>
				<MeleeWeapon_Cooldown>0.9</MeleeWeapon_Cooldown>
			</statFactors>
		</stuffProps>
		<isBodyPartOrImplant>true</isBodyPartOrImplant>
		<thingCategories>
			<li>WoodTypes</li>
		</thingCategories>
		<techHediffsTags>
			<li>Poor</li>
		</techHediffsTags>
		<verbs>
			<li>
				<verbClass>Verb_MeleeAttack</verbClass>
				<hasStandardCommand>true</hasStandardCommand>
				<meleeDamageDef>Blunt</meleeDamageDef>
			</li>
		</verbs>
		<butcherProducts>
			<LumberOak>30</LumberOak>
		</butcherProducts>
	</ThingDef>
  
